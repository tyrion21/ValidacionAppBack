generator client {
  provider      = "prisma-client-js"
  output        = "./generated/client5"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL4")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}